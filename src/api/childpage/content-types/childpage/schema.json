{
  "kind": "collectionType",
  "collectionName": "childpages",
  "info": {
    "singularName": "childpage",
    "pluralName": "childpages",
    "displayName": "Childpage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seoTitle": {
      "type": "string"
    },
    "seoDescription": {
      "type": "text"
    },
    "canonicalUrl": {
      "type": "string"
    },
    "keywords": {
      "type": "json"
    },
    "ogImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "jsonLd": {
      "type": "json"
    },
    "page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "childpages"
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "sections.text-left",
        "sections.text-center",
        "sections.section2",
        "sections.section1",
        "sections.item-section",
        "sections.hero"
      ]
    }
  }
}
